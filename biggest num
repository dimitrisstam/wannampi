#include <stdio.h>
int main()
{
	int a,b,c;
	int num;
	printf("Enter three numbers\n");
	scanf("%d %d %d",&a,&b,&c);
	if(a>b&&a>c)
	{
		num=a;
		printf("%d is the biggest number",a);
	}else if(b>a&&b>c)
	{
		num=b;
		printf("%d is the biggest number",b);
	}else if(c>a&&c>b)
	{
		num=c;
		printf("%d is the biggest number",c);
	}else
	{
		num=0;
		printf("ERROR");
	}
	return num;
}
